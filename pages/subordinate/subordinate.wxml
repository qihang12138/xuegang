<block wx:for="{{pageData.child}}" wx:key="{{index}}">
  <view class="flex-box subordinate bg-fff flex-align">
    <image src="{{item.image}}" class="subordinate-img"></image>
    <view class="flex-item">
      <view>{{item.nickname}}</view>
      <view class="font-12">
        ID:
        <text>{{item.code}}</text>
      </view>
    </view>
    <view class="font-12">
      佣金
      <text class="subordinate-text">{{item.integral}}</text>
    </view>
  </view>
</block>