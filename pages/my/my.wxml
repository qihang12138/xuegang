<view class="head-panel">
  <view class="avatar">
    <open-data type="userAvatarUrl"></open-data>
  </view>
  <open-data type="userNickName" class="head-name"></open-data>
</view>
<!-- 订单 -->
<view class="order">
  <view class="order-tit">我的订单</view>
  <view class="order-options">
    <navigator class="order-option" url="">
      <image class="order-img" src="https://s2.ax1x.com/2019/07/18/ZO4NDI.png"></image>
      <view>待付款</view>
    </navigator>
    <navigator class="order-option" url="">
      <image class="order-img" src="https://s2.ax1x.com/2019/07/18/ZO4NDI.png"></image>
      <view>待取货</view>
    </navigator>
    <navigator class="order-option" url="">
      <image class="order-img" src="https://s2.ax1x.com/2019/07/18/ZO4NDI.png"></image>
      <view>待评价</view>
    </navigator>
    <navigator class="order-option" url="">
      <image class="order-img" src="https://s2.ax1x.com/2019/07/18/ZO4NDI.png"></image>
      <view>已完成</view>
    </navigator>
    <navigator class="order-option" url="">
      <image class="order-img" src="https://s2.ax1x.com/2019/07/18/ZO4NDI.png"></image>
      <view>退货</view>
    </navigator>
  </view>
</view>
